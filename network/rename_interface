# File: /etc/udev/rules.d/80-net-name-slot.rules

# eth0 (replace ENVID_NET_NAME_PATH with name)
SUBSYSTEM=="net", AACTION=="add", DRIVERS=="?*", \
ENVID_NET_NAME_PATH=="enp12s0",	\
ATTRtype=="1", KERNEL=="eth*", NAME="eth0"
